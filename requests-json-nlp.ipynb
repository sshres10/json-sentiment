    "import requests\n",
    "import json\n",
    "\n",
    "def write_song_from_api_to_json(artist, song, filename):\n",
    "    # Fetch the lyrics\n",
    "    response = requests.get(f'https://api.lyrics.ovh/v1/{artist}/{song}')\n",
    "    data = response.json()\n",
    "    \n",
    "    # Save the lyrics to a file\n",
    "    with open(f\"{filename}.json\", \"w\") as file:\n",
    "        json.dump(data, file)\n",
    "    print(f\"Lyrics saved to {filename}.json\")\n",
    "\n",
    "# Test the function with four songs\n",
    "write_song_from_api_to_json(\"They Might Be Giants\", \"Birdhouse in your soul\", \"song1\")\n",
    "write_song_from_api_to_json(\"Adele\", \"Hello\", \"song2\")\n",
    "write_song_from_api_to_json(\"The Beatles\", \"Hey Jude\", \"song3\")\n",
    "write_song_from_api_to_json(\"Taylor Swift\", \"Shake it Off\", \"song4\")\n"
   ]
