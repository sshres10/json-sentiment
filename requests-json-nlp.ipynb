    "import requests\n",
    "import json\n",
    "import spacy\n",
    "from spacytextblob.spacytextblob import SpacyTextBlob\n",
    "\n",
    "# Fetch the lyrics from the API. we're fetching lyrics for \"Birdhouse in Your Soul\" by \"They Might Be Giants\"\n",
    "result = json.loads(requests.get('https://api.lyrics.ovh/v1/They Might Be Giants/Birdhouse in your soul').text)\n",
    "\n",
    "# Extract only the lyrics text from the API response\n",
    "lyrics = result.get('lyrics', '')  # Access only the lyrics part\n",
    "print(\"Lyrics:\", lyrics)\n",
    "\n",
    "# Load spaCy and add TextBlob for sentiment analysis\n",
    "nlp = spacy.load(\"en_core_web_sm\")\n",
    "nlp.add_pipe(\"spacytextblob\")\n",
    "\n",
    "# Process lyrics with spaCy pipeline and analyze sentiment\n",
    "doc = nlp(lyrics)\n",
    "polarity = doc._.blob.polarity  # Get polarity score (-1.0 to 1.0)\n",
    "print(\"Polarity Score:\", polarity)\n",
    "\n",
    "# Interpret the sentiment\n",
    "if polarity > 0:\n",
    "    print(\"The lyrics have a positive sentiment.\")\n",
    "elif polarity < 0:\n",
    "    print(\"The lyrics have a negative sentiment.\")\n",
    "else:\n",
    "    print(\"The lyrics have a neutral sentiment.\")\n"
   ]
