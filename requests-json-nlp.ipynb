    "import json\n",
    "\n",
    "def analyze_lyrics_sentiment(filename, song_name):\n",
    "    # Load the lyrics from the file\n",
    "    with open(f\"{filename}.json\", \"r\") as file:\n",
    "        data = json.load(file)\n",
    "        lyrics = data.get(\"lyrics\", \"\")  # Safely get lyrics part\n",
    "\n",
    "    # Perform sentiment analysis\n",
    "    doc = nlp(lyrics)\n",
    "    polarity = doc._.blob.polarity\n",
    "    \n",
    "    # Print the polarity score with the song name\n",
    "    print(f\"Polarity Score for '{song_name}':\", polarity)\n",
    "    \n",
    "    # Comment on the sentiment\n",
    "    if polarity > 0:\n",
    "        print(f\"The song '{song_name}' has a positive sentiment.\")\n",
    "    elif polarity < 0:\n",
    "        print(f\"The song '{song_name}' has a negative sentiment.\")\n",
    "    else:\n",
    "        print(f\"The song '{song_name}' has a neutral sentiment.\")\n",
    "\n",
    "# Analyze sentiment of each saved file\n",
    "analyze_lyrics_sentiment(\"song1\", \"Birdhouse in your soul\")\n",
    "analyze_lyrics_sentiment(\"song2\", \"Hello\")\n",
    "analyze_lyrics_sentiment(\"song3\", \"Hey Jude\")\n",
    "analyze_lyrics_sentiment(\"song4\", \"Shake it Off\")\n",
    "\n"
